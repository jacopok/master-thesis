@article{kumarAccuracyPrecisionGravitationalwave2015,
  title = {Accuracy and Precision of Gravitational-Wave Models of Inspiraling Neutron Star -- Black Hole Binaries with Spin: Comparison with Numerical Relativity in the Low-Frequency Regime},
  shorttitle = {Accuracy and Precision of Gravitational-Wave Models of Inspiraling Neutron Star -- Black Hole Binaries with Spin},
  author = {Kumar, Prayush and Barkett, Kevin and Bhagwat, Swetha and Afshari, Nousha and Brown, Duncan A. and Lovelace, Geoffrey and Scheel, Mark A. and Szilágyi, Béla},
  date = {2015-11-04},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume = {92},
