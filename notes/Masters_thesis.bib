@article{radiceProbingExtremeDensityMatter2017,
  title = {Probing {{Extreme}}-{{Density Matter}} with {{Gravitational Wave Observations}} of {{Binary Neutron Star Merger Remnants}}},
  author = {Radice, David and Bernuzzi, Sebastiano and Del Pozzo, Walter and Roberts, Luke F. and Ott, Christian D.},
  date = {2017-06-14},
