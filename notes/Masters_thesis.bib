}

@article{lackeyEffectiveonebodyWaveformsBinary2017,
  title = {Effective-One-Body Waveforms for Binary Neutron Stars Using Surrogate Models},
  author = {Lackey, Benjamin D. and Bernuzzi, Sebastiano and Galley, Chad R. and Meidam, Jeroen and Broeck, Chris Van Den},
  date = {2017-05-30},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume = {95},
  number = {10},
  eprint = {1610.04742},
  eprinttype = {arxiv},
  pages = {104036},
  issn = {2470-0010, 2470-0029},
  doi = {10.1103/PhysRevD.95.104036},
  url = {http://arxiv.org/abs/1610.04742},
  urldate = {2020-11-20},
  abstract = {Gravitational-wave observations of binary neutron star systems can provide information about the masses, spins, and structure of neutron stars. However, this requires accurate and computationally efficient waveform models that take {$<$}1s to evaluate for use in Bayesian parameter estimation codes that perform 10\^7 - 10\^8 waveform evaluations. We present a surrogate model of a nonspinning effective-one-body waveform model with l = 2, 3, and 4 tidal multipole moments that reproduces waveforms of binary neutron star numerical simulations up to merger. The surrogate is built from compact sets of effective-one-body waveform amplitude and phase data that each form a reduced basis. We find that 12 amplitude and 7 phase basis elements are sufficient to reconstruct any binary neutron star waveform with a starting frequency of 10Hz. The surrogate has maximum errors of 3.8\% in amplitude (0.04\% excluding the last 100M before merger) and 0.043 radians in phase. The version implemented in the LIGO Algorithm Library takes \textasciitilde 0.07s to evaluate for a starting frequency of 30Hz and \textasciitilde 0.8s for a starting frequency of 10Hz, resulting in a speed-up factor of \textasciitilde 10\^3 - 10\^4 relative to the original Matlab code. This allows parameter estimation codes to run in days to weeks rather than years, and we demonstrate this with a Nested Sampling run that recovers the masses and tidal parameters of a simulated binary neutron star system.},
  archiveprefix = {arXiv},
  keywords = {General Relativity and Quantum Cosmology,To read},
  file = {/home/jacopo/zotero/storage/DIHXWV68/Lackey et al. - 2017 - Effective-one-body waveforms for binary neutron st.pdf;/home/jacopo/zotero/storage/4YH3C9V6/1610.html}
}

@article{lackeySurrogateModelAlignedspin2019,
  title = {Surrogate Model for an Aligned-Spin Effective One Body Waveform Model of Binary Neutron Star Inspirals Using {{Gaussian}} Process Regression},
  author = {Lackey, Benjamin D. and Pürrer, Michael and Taracchini, Andrea and Marsat, Sylvain},
  date = {2019-07-03},
