@article{damourMeasurabilityTidalPolarizability2012,
  title = {Measurability of the Tidal Polarizability of Neutron Stars in Late-Inspiral Gravitational-Wave Signals},
  author = {Damour, Thibault and Nagar, Alessandro and Villain, Loic},
  date = {2012-06-15},
