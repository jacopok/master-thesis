}

@online{gambaWaveformSystematicsGravitationalwave2020,
  title = {Waveform Systematics in the Gravitational-Wave Inference of Tidal Parameters and Equation of State from Binary Neutron Star Signals},
  author = {Gamba, Rossella and Breschi, Matteo and Bernuzzi, Sebastiano and Agathos, Michalis and Nagar, Alessandro},
  date = {2020-09-17},
  url = {http://arxiv.org/abs/2009.08467},
  urldate = {2020-11-13},
  abstract = {Gravitational-wave signals from binary neutron star coalescences carry information about the star's equation of state in their tidal signatures. A major issue in the inference of the tidal parameters (or directly of the equation of state) is the systematic error introduced by the waveform approximants. We use a bottom-up approach based on gauge-invariant phase analysis and the Fisher information matrix to investigate waveform systematics and help identifying biases in parameter estimation. A mock analysis of 15 different binaries indicates that systematics in current waveform models dominate over statistical errors at signal-to-noise ratio (SNR) \$\{\textbackslash gtrsim\} 80\$. This implies biases in the inference of the reduced tidal parameter that are are larger than the statistical \$90\textbackslash\%\$ credible-intervals. For example, while the neutron-star radius could be constrained at \$\{\textbackslash sim\} 5\textbackslash\%\$ level at SNR 80, systematics can be at the \$\{\textbackslash sim\} 10\textbackslash\%\$ level. We apply our approach to GW170817 (SNR \$\{\textbackslash sim\}30\$) and confirm that no significant systematic effects are present. Using an optimal frequency range for the analysis, we estimate a neutron-star radius of \$12.5\^\{+1.1\}\_\{-1.8\}\textbackslash,\$km. The latter is consistent with an electromagnetic-informed prior and the recent NICER measurement. Exploring SNR \$\{\textbackslash gtrsim\}100\$ in view of third-generation detectors, we find that all the current waveform models lead to differences of at least 1-sigma in the inference of the reduced tidal parameter (for any value of the latter). We conclude that current waveform models, including those from numerical relativity, are insufficient to infer the equation of state in the loudest (and potentially most informative) events that will be observed by advanced and third generation detectors.},
  archiveprefix = {arXiv},
  eprint = {2009.08467},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/C8JD7NV2/Gamba et al. - 2020 - Waveform systematics in the gravitational-wave inf.pdf;/home/jacopo/zotero/storage/YDY9T9DR/2009.html},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  primaryclass = {astro-ph, physics:gr-qc}
}

