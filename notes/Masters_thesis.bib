  eprint = {1507.00103},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/SCA2KQ4M/Kumar et al_2015_Accuracy and precision of gravitational-wave models of inspiraling neutron star.pdf;/home/jacopo/zotero/storage/NG2343VB/1507.html},
