@online{gambaFastFaithfulFrequencydomain2020,
  title = {Fast, Faithful, Frequency-Domain Effective-One-Body Waveforms for Compact Binary Coalescences},
  author = {Gamba, Rossella and Bernuzzi, Sebastiano and Nagar, Alessandro},
  date = {2020-11-30},
